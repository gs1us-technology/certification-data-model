{
  "@context": {
    "gs1": "https://gs1.org/voc/",
    "gs1x": "https://gs1.org/voc/x/",
    "xsd": "http://www.w3.org/2001/XMLSchema#",

    "id": "@id",
    "type": "@type",

    "CertificationReport": "gs1x:CertificationReport",
    "Product": "gs1:Product",
    "CertificationDetails": "gs1:CertificationDetails",
    "Country": "gs1:Country",

    "issueDate": {
      "@id": "gs1x:issueDate",
      "@type": "xsd:date"
    },
    "issuer": {
      "@id": "gs1x:issuer",
      "@type": "xsd:string"
    },
    "products": {
      "@id": "gs1x:products",
      "@type": "@vocab",
      "@container": "@set"
    },

    "gtin": {
      "@id": "gs1:gtin"
    },
    "description": {
      "@id": "gs1:productDescription"
    },
    "name": {
      "@id": "gs1:productName"
    },
    "additionalTradeItemId": {
      "@id": "gs1:productID"
    },
    "certificationInfo": {
      "@id": "gs1:certification"
    },
    "agencyURL" : {
      "@id": "gs1:certificationAgencyURL"
    },
    "agency": "gs1:certificationAgency",
    "endDate": {
      "@id": "gs1:certificationEndDate"
    },
    "startDate": {
      "@id": "gs1:certificationStartDate"
    },
    "standard": "gs1:certificationStandard",

    "status": {
      "@context": {
        "ACTIVE": "https://ref.gs1.org/voc/CertificationStatus-ACTIVE",
        "INACTIVE": "https://ref.gs1.org/voc/CertificationStatus-INACTIVE"
      },
      "@id": "gs1:certificationStatus",
      "@type": "@vocab"
    },

    "certificateId" : "gs1:certificationIdentification",
    "program": {
      "@id": "gs1x:certificationProgram",
      "@type" : "rdf:langString"
    },
    "value": "gs1:certificationValue",

    "targetCountries": {
      "@id": "gs1x:certificationApplicableCountries"
    },

    "countryCode": {
      "@id": "gs1:countryCode"
    }
  }
}
